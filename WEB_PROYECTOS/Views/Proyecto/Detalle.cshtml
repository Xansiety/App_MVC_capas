@model Entidad.Proyecto

@{
    ViewBag.Title = "Detalle";
}

<h2>Detalle</h2>

<div>
    <h4>Proyecto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre del proyecto
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NombreProyecto)
        </dd>

        <dt>
          Fecha Inicio proyecto
        </dt>

        <dd>
            @*@Html.DisplayFor(model => model.FechaInicio)*@
            @Model.FechaInicio.ToShortDateString()
        </dd>

        <dt>
           Fecha Fin proyecto
        </dt>

        <dd>
            @Model.FechaFin.ToShortDateString()
        </dd>

    </dl>
</div>
<p>
    <a class="btn btn-primary" href="@Url.Action("Editar","Proyecto", new { id = Model.ProyectoId })"> Editar </a>
    <a class="btn btn-success" href="@Url.Action("Index","Proyecto")"> Lista </a>

</p>
